{
  "name": "RepackHostApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle-ios": "react-native bundle --config rspack.config.mjs --platform ios --entry-file index.js --dev=false --bundle-output build/generated/ios/index.bundle --assets-dest build/generated/ios",
    "bundle-android": "react-native bundle --config rspack.config.mjs --platform android --entry-file index.js --dev=false --bundle-output build/generated/android/index.bundle --assets-dest build/generated/android"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.5",
    "@react-navigation/native-stack": "^7.3.9",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-reanimated": "^3.17.2",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "^5.0.6",
    "@eslint/js": "^9.24.0",
    "@module-federation/enhanced": "^0.11.3",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@rspack/cli": "^1.3.3",
    "@rspack/core": "^1.2.8",
    "@swc/helpers": "^0.5.15",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.29.1"
  },
  "engines": {
    "node": ">=18"
  }
}
